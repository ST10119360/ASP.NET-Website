@{
    ViewBag.Title = "Home Page";
}
<body style=" background-image: url(../Assets/new.png)">
    <div class="jumbotron" style="background-color:lightseagreen">
        <h1 style="color: black">ST10119360 The Ride You Rent CRUD</h1>
        <p class="lead" style="color:black">This is The Ride You rent CRUD website where we can edit, create and delete data for the company. Only Inspectors can access the tables</p>

    </div>
</body>

<div style="background-color:lightseagreen">

    @Html.ActionLink("Car Table", "Index", "Cars", new { id = 1 }, 
            new { @style = "color:#000000" }
            )<br />
    @Html.ActionLink("Driver Table", "Index", "Drivers", new { id = 1 },
            new { @style = "color:#000000" }
            )<br />
    @Html.ActionLink("Inspector Table", "Index", "Inspectors", new { id = 1 },
            new { @style = "color:#000000" }
            
            )<br />
    @Html.ActionLink("Rental Table", "Index", "Rentals", new { id = 1 },
            new { @style = "color:#000000" }
            )<br />
    @Html.ActionLink("Returned Cars Table", "Index", "ReturnCars", new { id = 1 },
            new { @style = "color:#000000" }
            )<br />
</div>








